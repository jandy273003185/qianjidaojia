<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qifenqian.bms.myworkspace.ActViewUrlMapper">

	<select id="selectByApplicationName" parameterType="string" resultType="com.qifenqian.bms.myworkspace.ActViewUrl">
		SELECT * 
		FROM 
			act_view_url A
		WHERE
			A.application_name = #{applicationName,jdbcType=VARCHAR}
	</select>
</mapper>