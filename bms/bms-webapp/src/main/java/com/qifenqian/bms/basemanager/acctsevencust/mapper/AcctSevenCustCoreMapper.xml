<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qifenqian.bms.basemanager.acctsevencust.mapper.AcctSevenCustCoreMapper">

	<select id="updateAccountName" parameterType="com.qifenqian.bms.basemanager.acctsevencust.bean.AcctSevenCust">
		update stcs.acct_seven_cust  set acct_name=#{acctName,jdbcType = VARCHAR} where cust_id=#{custId,jdbcType = VARCHAR}
	</select>
	
</mapper>