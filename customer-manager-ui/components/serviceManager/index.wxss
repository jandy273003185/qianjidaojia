/* components/searchCommercialTenant/index.wxss */
page{
  background-color: #eeeeee;
  overflow-y: hidden;
}
.deviceScroll{
  overflow:scroll;
  /* height: 500rpx; */
  /* border: 1px solid red; */
}
.navbar{
  display: flex;
  align-items: center;
  background-color: #699dd7;
  width: 100%;
  height: 100rpx;
  padding-top: 40rpx;
  padding-left: 10rpx;
  position: fixed;
  z-index: 999;
}
.navbar image{
  width: 22rpx;
  height: 32rpx;
  padding: 8rpx 20rpx 0 20rpx;
}
.navbar text{
  color: #fff !important;
  font-size: 38rpx !important;
}

